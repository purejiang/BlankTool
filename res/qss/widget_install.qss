QWidget#widget_install_fuction_bar {
  border-radius: 5px;
  background-color: #595959;
}

QPushButton#btn_select_install{ 
  border-radius: 5px;
  border: 1px solid #FFFFFF;
  background-color: transparent;
  color: #FFFFFF;
  font-family: "Microsoft YaHei";
  font-size: 12px;
}
QPushButton#btn_select_install:default {
  background-color: transparent;
}

QPushButton#btn_select_install:hover{
  background-color: #64C8FE;
}

QLineEdit {
  background: #FFFFFF;
  border-radius: 4px;
  border: 1px solid #FFFFFF;
  font-family: "Microsoft YaHei";
  font-size: 14px;
  color: #3D3D3D;
}

QListWidget {
    border-radius: 5px;
    border: 0px;
    outline: 0px;
    background-color: #595959;
}

QListWidget::Item {
    background: transparent;
    color: transparent;
    min-width: 700px;
    min-height: 93px;
    font-family: "Microsoft YaHei";
    font-size: 12px;
    font-weight: bold;
    border-radius: 5px;
}

QListWidget::Item:hover {
    background: #646464;
}

QListWidget::Item:selected {
    background: #4e4e4e;
}

